{
  "name": "scalable-socket",
  "version": "0.1.0",
  "author": "Maciej Cieslar",
  "description": "Scalable websockets with Nest and Redis.",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.5",
    "@nestjs/core": "^6.11.5",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^6.11.5",
    "@nestjs/websockets": "^6.11.5",
    "dotenv": "^8.2.0",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/schematics": "^6.9.1",
    "@nestjs/testing": "^6.11.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/ioredis": "^4.14.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.3",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  }
}
